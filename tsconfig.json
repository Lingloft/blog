{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "types": [
            "node",
            "vue",
            "webpack-env"
        ],
        "baseUrl": "."
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}